using System;

public class Program
{
	public static void Main()
	{
		Random r = new Random();
		
		int p = 0;
		int p2= 100;
		
		int N = r.Next(1,100);
		int N2 = r.Next(p,p2);
		
		while(N2 != N){
			if(N2< N){
				N2++;
				Console.WriteLine("Mais alto");
			}
			if(N2> N){
				N2--;
				Console.WriteLine("Mais baixo");
			}
			
			if(N2 == N){
				
			}
		
		}

	}	
}
